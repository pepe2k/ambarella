config BUILD_AMBARELLA_BSREADER_PACKAGE
	bool "Build Ambarella bsreader library (bit stream reader)"
	depends on (CONFIG_ARCH_A5S || CONFIG_ARCH_A7 || CONFIG_ARCH_A7L || CONFIG_ARCH_S2)
	default n
	help
		Build Ambarella bsreader library

