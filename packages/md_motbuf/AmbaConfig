config BUILD_AMBARELLA_MDMOTBUF_PACKAGE
	bool "Build Ambarella motion detect library (using motion buffer)"
	depends on (CONFIG_ARCH_A5S || CONFIG_ARCH_S2)
	default n
	help
		Build Ambarella motion detect library

