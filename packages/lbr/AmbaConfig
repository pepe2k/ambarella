config BUILD_AMBARELLA_LBR_PACKAGE
	bool "Build Ambarella LBR (Low Bitrate) library"
	depends on (CONFIG_ARCH_A5S || CONFIG_ARCH_S2 ||  CONFIG_ARCH_S2L)
	default n
	help
		Build Ambarella Low Bitrate library

