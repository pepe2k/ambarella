menu "Ambarella Keymap"
config CONFIG_AMBARELLA_KEYMAP
	depends on CONFIG_ARCH_A5S || CONFIG_ARCH_S2
	bool "Configure to include Ambarella Keymap"
	default n
	help
		Configure to include Ambarella Keymap.

endmenu

