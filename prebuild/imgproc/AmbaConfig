config CONFIG_AMBARELLA_IMAGE_DATA
	depends on CONFIG_ARCH_A5S || CONFIG_ARCH_S2 || CONFIG_ARCH_S2L
	bool "Configure to include Ambarella Image Data"
	default n
	help
		Configure to include Ambarella Image Data.
