config BUILD_AMBARELLA_FDET
depends on BUILD_AMBARELLA_PRIVATE_MODULES && (CONFIG_ARCH_A7 || CONFIG_ARCH_S2)
	bool "Build Ambarella private Fdet modules"
	default n
	help
		Build Ambarella private face detection modules for Linux.
