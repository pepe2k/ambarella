config BUILD_AMBARELLA_DSP_LOG
depends on BUILD_AMBARELLA_PRIVATE_MODULES
	bool "Build Ambarella DSP Log Driver module"
	default n
	help
		Build Ambarella DSP Log Driver (Independant to DSP Driver).

